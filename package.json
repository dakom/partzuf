{
  "name": "partzuf",
  "version": "3.0.2",
  "description": "partzuf",
  "main": "dist/lib.cjs.js",
  "module": "dist/lib.esm.js",
  "typings": "./dist/typings/lib.d.ts",
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./doc",
    "//dev": "npm-run-all -s clean -p rollup:watch",
    "dev": "npm-run-all -s clean -p test:watch",
    "build": "npm-run-all clean rollup:build typings:emit doc",
    "typings:emit": "tsc --emitDeclarationOnly true",
    "rollup:build": "cross-env NODE_ENV=production rollup -c",
    "rollup:watch": "cross-env NODE_ENV=production rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch --no-cache",
    "test:clear": "jest --clearCache",
    "doc": "typedoc --out ./doc/ ./src/lib/lib.ts",
    "doc:serve": "http-server ./doc --open"
  },
  "author": "David Komer",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.2",
    "chokidar": "3.0.2",
    "cross-env": "5.2.0",
    "glob": "7.1.4",
    "jest": "24.9.0",
    "minify": "4.1.3",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.0",
    "rollup": "1.19.4",
    "rollup-plugin-commonjs": "10.0.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-typescript2": "0.22.1",
    "rollup-plugin-uglify": "6.0.2",
    "shelljs": "0.8.3",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tslib": "1.10.0",
    "typedoc": "^0.15.0",
    "typescript": "3.5.3",
    "uglify-es": "3.3.9"
  },
  "dependencies": {
    "fp-ts": "^2.0.5"
  }
}
